{
    "manifest_version": 2,
    "name": "2ManyTabz",
    "description": "This extension allows you to organize your tabs!",
    "version": "1.0",
    "content_scripts": [
        {
        "matches": ["https://www.google.com/*"],
        "css": ["main.css"]
        }
        ],
    "browser_action": {
        "default_title": "My personalized google page!",
        "default_popup": "popup.html"
        },
    "icons": { "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png" }
   }